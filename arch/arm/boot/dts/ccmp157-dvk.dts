/* 10/100/1000 Ethernet */
&ethernet0 {
	status = "okay";
	pinctrl-0 = <&ccmp15_ethernet0_rgmii_pins>;
	pinctrl-1 = <&ccmp15_ethernet0_rgmii_sleep_pins>;
	pinctrl-names = "default", "sleep";
	phy-mode = "rgmii-id";
	max-speed = <1000>;
	phy-handle = <&phy0>;
	local-mac-address = [00 04 F3 22 23 24];
	st,eth-clk-sel = <1>; /* Internal 125 MHz clock */
	clock-names = "stmmaceth",
				  "mac-clk-tx",
				  "mac-clk-rx",
				  "eth-ck",
				  "ethstp";
	clocks = <&rcc ETHMAC>,
			 <&rcc ETHTX>,
			 <&rcc ETHRX>,
			 <&rcc ETHCK_K>,
			 <&rcc ETHSTP>;

	mdio0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "snps,dwmac-mdio";

		phy0: ethernet-phy@0 {
			reg = <0>;
			compatible = "ethernet-phy-id0283.bc30"; /* PHY ID for ADIN1300 */
			reset-gpios = <&gpiog 0 (GPIO_ACTIVE_LOW | GPIO_PULL_UP)>;
			reset-assert-us = <1000>;
			reset-deassert-us = <2000>;
			interrupt-parent = <&gpiog>; /* ETH_INT */
			interrupts = <5 IRQ_TYPE_LEVEL_LOW>;
		};
	};
};

&hash1 {
	status = "okay";
};

/* Connected to XBee some pins are shared with LTDC */
&usart3 {
	pinctrl-names = "default", "sleep", "idle";
	pinctrl-0 = <&ccmp15_usart3_pins>;
	pinctrl-1 = <&ccmp15_usart3_sleep_pins>;
	pinctrl-2 = <&ccmp15_usart3_idle_pins>;
	uart-has-rtscts;
	status = "okay";
};

&i2s2 {
	#clock-cells = <0>;
	clocks = <&rcc SPI2>, <&rcc SPI2_K>, <&rcc PLL3_Q>, <&rcc PLL3_R>;
	clock-names = "pclk", "i2sclk", "x8k", "x11k";
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&ccmp15_i2s2_pins_a>;
	pinctrl-1 = <&ccmp15_i2s2_sleep_pins_a>;
	status = "okay";
};

